{
  "name": "cjs2esm",
  "description": "Transforms a project that uses CommonJS to ES Modules.",
  "homepage": "https://homer0.github.io/cjs2esm/",
  "version": "1.1.0",
  "repository": "homer0/cjs2esm",
  "author": "Leonardo Apiwan (@homer0) <me@homer0.com>",
  "keywords": [
    "commonjs",
    "esmodules",
    "cjs",
    "esm",
    "es-modules"
  ],
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "5to6-codemod": "1.8.0",
    "chalk": "^4.1.0",
    "del": "^5.1.0",
    "fs-extra": "^9.0.1",
    "jscodeshift": "0.10.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.6.0",
    "eslint-plugin-homer0": "^5.0.1",
    "husky": "^4.2.5",
    "jest": "^26.2.2",
    "jimple": "^1.5.0",
    "jsdoc": "^3.6.5",
    "jsdoc-ts-utils": "^1.0.0",
    "docdash": "homer0/docdash#semver:^2.1.0",
    "leasot": "^11.1.0",
    "lint-staged": "^10.2.11",
    "semantic-release": "^17.1.1"
  },
  "main": "src/index.js",
  "bin": {
    "cjs2esm": "./src/bin.js"
  },
  "engine-strict": true,
  "engines": {
    "node": ">=10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "./utils/hooks/pre-commit",
      "post-merge": "./utils/hooks/post-merge",
      "prepare-commit-msg": "./utils/hooks/prepare-commit-msg"
    }
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "scripts": {
    "prepublishOnly": "./utils/scripts/prepublishOnly",
    "test": "./utils/scripts/test",
    "lint": "./utils/scripts/lint",
    "lint:all": "./utils/scripts/lint-all",
    "docs": "./utils/scripts/docs",
    "todo": "./utils/scripts/todo"
  },
  "config": {
    "cjs2esm": {
      "filesWithShebang": [
        "src/bin.js"
      ]
    },
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
