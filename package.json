{
  "name": "cjs2esm",
  "description": "Transforms a project that uses CommonJS to ES Modules.",
  "homepage": "https://homer0.github.io/cjs2esm/",
  "version": "1.1.2",
  "repository": "homer0/cjs2esm",
  "author": "Leonardo Apiwan (@homer0) <me@homer0.com>",
  "keywords": [
    "commonjs",
    "esmodules",
    "cjs",
    "esm",
    "es-modules"
  ],
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "5to6-codemod": "1.8.0",
    "chalk": "^4.1.0",
    "del": "^6.0.0",
    "fs-extra": "^9.1.0",
    "jscodeshift": "0.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "eslint": "^7.24.0",
    "eslint-plugin-homer0": "^5.0.2",
    "husky": "^6.0.0",
    "is-ci": "^3.0.0",
    "jest": "^26.6.3",
    "jimple": "^1.5.0",
    "jsdoc": "^3.6.6",
    "jsdoc-ts-utils": "^1.1.2",
    "docdash": "homer0/docdash#semver:^2.1.0",
    "leasot": "^11.6.0",
    "lint-staged": "^10.5.4",
    "semantic-release": "^17.4.2"
  },
  "main": "src/index.js",
  "bin": {
    "cjs2esm": "./src/bin.js"
  },
  "engine-strict": true,
  "engines": {
    "node": ">=12"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "scripts": {
    "prepublishOnly": "./utils/scripts/prepublishOnly",
    "test": "./utils/scripts/test",
    "lint": "./utils/scripts/lint",
    "lint:all": "./utils/scripts/lint-all",
    "docs": "./utils/scripts/docs",
    "todo": "./utils/scripts/todo",
    "prepare": "./utils/scripts/prepare"
  },
  "config": {
    "cjs2esm": {
      "filesWithShebang": [
        "src/bin.js"
      ]
    }
  }
}
