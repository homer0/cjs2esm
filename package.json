{
  "name": "cjs2esm",
  "description": "Transforms a project that uses CommonJS to ES Modules.",
  "homepage": "https://homer0.github.io/cjs2esm/",
  "version": "0.0.0-development",
  "repository": "homer0/cjs2esm",
  "author": "Leonardo Apiwan (@homer0) <me@homer0.com>",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.6.0",
    "eslint-plugin-homer0": "^5.0.1",
    "husky": "^4.2.5",
    "jest": "^26.2.2",
    "jimple": "^1.5.0",
    "jsdoc": "^3.6.5",
    "jsdoc-ts-utils": "^1.0.0",
    "docdash": "homer0/docdash#semver:^2.0.0",
    "leasot": "^11.1.0",
    "lint-staged": "^10.2.11",
    "semantic-release": "^17.1.1"
  },
  "engine-strict": true,
  "engines": {
    "node": ">=10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "./utils/hooks/pre-commit",
      "post-merge": "./utils/hooks/post-merge",
      "prepare-commit-msg": "./utils/hooks/prepare-commit-msg"
    }
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "scripts": {
    "test": "./utils/scripts/test",
    "lint": "./utils/scripts/lint",
    "lint:all": "./utils/scripts/lint-all",
    "docs": "./utils/scripts/docs",
    "todo": "./utils/scripts/todo"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
